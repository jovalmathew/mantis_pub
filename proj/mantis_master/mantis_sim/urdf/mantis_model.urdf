	<axis xyz="0 1 0"/>
	<limit effort="1000.0" lower="-1.5" upper="1.5" velocity="0.25"/>
	<origin rpy="0 0 0" xyz="-0.04 0 0.2588"/>
	<parent link="joint_three_housing_A"/>
	<child link="joint_three_housing_B"/>
</joint>

<!-- ======================= -->
<!-- Link 3 -->
<!-- ======================= -->
